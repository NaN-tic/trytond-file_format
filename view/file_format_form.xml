<form col="6">
    <label name="name"/>
    <field name="name"/>
    <label name="model"/>
    <field name="model"/>
    <label name="file_type"/>
    <field name="file_type"/>
    <label name="engine"/>
    <field name="engine"/>
    <label name="path"/>
    <field name="path"/>
    <label name="file_name"/>
    <field name="file_name"/>
    <label name="state"/>
    <field name="state"/>
    <notebook colspan="6">
        <page string="CSV" id="csv_fields">
            <group id="flag_fields" colspan="4" col="6">
                <label name="header"/>
                <field name="header"/>
                <label name="separator"/>
                <field name="separator"/>
                <label name="quote"/>
                <field name="quote"/>
            </group>
            <field name="ffields" colspan="4"/>
        </page>
        <page string="XML" name="xml_format">
            <field name="xml_format"/>
        </page>
    </notebook>
</form>
